<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="xsl/pts-exdep-viewer.xsl" ?>
<PhoronixTestSuite>
	<ExternalDependencies>
		<Information>
			<Name>Optware</Name>
			<Aliases>Palm, HP</Aliases>
		</Information>
		<Package>
			<GenericName>gtk-development</GenericName>
			<PackageName>gtk</PackageName>
			<FileCheck>/opt/include/gtk-2.0</FileCheck>
		</Package>
		<Package>
			<GenericName>sdl-development</GenericName>
			<PackageName>sdl sdl-devel</PackageName>
			<FileCheck>/opt/include/SDL/SDL.h</FileCheck>
		</Package>
		<Package>
			<GenericName>smartmontools</GenericName>
			<PackageName>smartmontools</PackageName>
		</Package>
		<Package>
			<GenericName>csh</GenericName>
			<PackageName>tcsh</PackageName>
			<FileCheck>/opt/bin/tcsh</FileCheck>
		</Package>
		<Package>
			<GenericName>libpng-development</GenericName>
			<PackageName>libpng</PackageName>
			<FileCheck>/opt/include/libpng12/png.h</FileCheck>
		</Package>
		<Package>
			<GenericName>build-utilities</GenericName>
			<PackageName>gcc make automake</PackageName>
			<FileCheck>/opt/bin/gcc, /opt/bin/make</FileCheck>
		</Package>
		<Package>
			<GenericName>bison</GenericName>
			<PackageName>bison</PackageName>
			<FileCheck>/opt/bin/bison</FileCheck>
		</Package>
		<Package>
			<GenericName>flex</GenericName>
			<PackageName>flex</PackageName>
			<FileCheck>/opt/bin/flex</FileCheck>
		</Package>
		<Package>
			<GenericName>scons</GenericName>
			<PackageName>scons</PackageName>
			<FileCheck>/opt/bin/scons</FileCheck>
		</Package>
		<Package>
			<GenericName>zlib-development</GenericName>
			<PackageName>zlib</PackageName>
			<FileCheck>/opt/include/zlib.h</FileCheck>
		</Package>
		<Package>
			<GenericName>jpeg-development</GenericName>
			<PackageName>libjpeg</PackageName>
			<FileCheck>/opt/include/jpeglib.h</FileCheck>
		</Package>
		<Package>
			<GenericName>perl</GenericName>
			<PackageName>perl</PackageName>
			<FileCheck>/opt/bin/perl</FileCheck>
		</Package>
		<Package>
			<GenericName>vorbis-development</GenericName>
			<PackageName>libvorbis</PackageName>
			<FileCheck>/opt/include/vorbis/vorbisfile.h</FileCheck>
		</Package>
		<Package>
			<GenericName>jam</GenericName>
			<PackageName>jamvm</PackageName>
			<FileCheck>/opt/bin/jam</FileCheck>
		</Package>
		<Package>
			<GenericName>autoconf</GenericName>
			<PackageName>autoconf</PackageName>
			<FileCheck>/opt/bin/autoconf</FileCheck>
		</Package>
		<Package>
			<GenericName>libtool</GenericName>
			<PackageName>libtool</PackageName>
			<FileCheck>/opt/bin/libtool</FileCheck>
		</Package>
		<Package>
			<GenericName>ncurses-development</GenericName>
			<PackageName>ncurses</PackageName>
			<FileCheck>/opt/include/ncurses.h</FileCheck>
		</Package>
		<Package>
			<GenericName>popt</GenericName>
			<PackageName>popt</PackageName>
			<FileCheck>/opt/include/popt.h</FileCheck>
		</Package>
		<Package>
			<GenericName>curl</GenericName>
			<PackageName>libcurl libcurl-dev</PackageName>
			<FileCheck>/opt/lib/libcurl.so.3</FileCheck>
		</Package>
		<Package>
			<GenericName>boost-development</GenericName>
			<PackageName>boot-date-time boost-dev boost-system boost-thread boost-signals boost-regex</PackageName>
			<FileCheck>/opt/include/boost/</FileCheck>
		</Package>
		<Package>
			<GenericName>bzip2-development</GenericName>
			<PackageName>bzip2</PackageName>
			<FileCheck>/opt/include/bzlib.h</FileCheck>
		</Package>
		<Package>
			<GenericName>tcl</GenericName>
			<PackageName>tcl</PackageName>
			<FileCheck>/opt/bin/tclsh</FileCheck>
		</Package>
		<Package>
			<GenericName>glibc-development</GenericName>
			<PackageName>libc-dev</PackageName>
			<FileCheck>/opt/lib/libc.a</FileCheck>
		</Package>
		<Package>
			<GenericName>python</GenericName>
			<PackageName>python</PackageName>
			<FileCheck>/opt/bin/python</FileCheck>
		</Package>
	</ExternalDependencies>
</PhoronixTestSuite>
